{
    "name": "devarts/plausible-api",
    "description": "A PHP wrapper for using Plausible's API",
    "keywords": [
        "plausible", "api", "wrapper", "client"
    ],
    "homepage": "https://github.com/devarts/plausible-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Marko Ognjenovic",
            "email": "marko.ognjenovicgm@gmail.com",
            "homepage": "https://github.com/devarts",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.4",
        "guzzlehttp/guzzle": "^6.5",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit" : "7.*"
    },
    "autoload": {
        "psr-4": {
            "Plausible\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Plausible\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
