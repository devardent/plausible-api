{
    "name": "devarts/plausible-php",
    "description": "A PHP wrapper for using Plausible API",
    "keywords": [
        "plausible", "api", "wrapper", "client", "php"
    ],
    "homepage": "https://github.com/devarts/plausible-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Marko Ognjenovic",
            "email": "marko.ognjenovicgm@gmail.com",
            "homepage": "https://github.com/devarts",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.4",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit" : "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Plausible\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Plausible\\Test\\": "tests"
        }
    }
}
